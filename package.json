{
  "name": "@vinfo/node-npm-view",
  "type": "module",
  "version": "0.0.6",
  "description": "download .tgz that package-lock.json by node.js",
  "author": "benyuanzhang",
  "license": "Apache-2.0",
  "bin": {
    "tgz": "bin/index.mjs"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/qwecxzpoi/node-npm-view#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwecxzpoi/node-npm-view.git"
  },
  "bugs": {
    "url": "https://github.com/qwecxzpoi/node-npm-view/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "esno ./src/index.ts",
    "build": "rimraf dist && unbuild",
    "dev": "pnpm build && pnpm i -g ~/project/node-npm-view/",
    "typecheck": "tsc",
    "release": "bumpp && pnpm publish --no-git-checks",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "download tgz",
    "package-lock.json"
  ],
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  }
}
